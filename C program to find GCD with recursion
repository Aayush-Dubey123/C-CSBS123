#include <stdio.h>
int gcd(int a,int b)
{
    if(b==0)
    {
        return(a);
    }
    else
    {
        return gcd(b,a%b);
    }
}
int main()
{
    int x,y;
    printf("\nenter two numbers=");
    scanf("%d%d",&x,&y);
    printf("\ngcd of %d and %d is %d ",x,y,gcd(x,y));
    return(0);
}
output=
enter two numbers=48 36
gcd of 48 and 36 is 12 
