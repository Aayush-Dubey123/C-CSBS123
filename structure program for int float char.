// Online C compiler to run C program online
#include <stdio.h>
struct ABC
{
    int a;
    float b;
    char c;
}A1;

int main() {
   struct ABC A2;
   A1.a = 10 ;
   A1.b = 22.5 ;
   A1.c = 'H' ;
    A2.a = 12 ;
   A2.b = 24.5 ;
   A2.c = 'A' ;
   
    printf("\n %d \n %f \n %c",A1.a,A1.b,A1.c);
    printf("\n %d \n %f \n %c",A2.a,A2.b,A2.c);
    return 0;
}
output =
